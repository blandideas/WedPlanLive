# Environment settings
NODE_ENV=production

# Server configuration
PORT=8080

# Database connection string
# For Cloudways PostgreSQL database:
# Replace with your actual Cloudways database credentials
DATABASE_URL=postgresql://username:password@host:port/dbname

# If you're using SSL for database connection (recommended):
DATABASE_SSL=true

# Session secret (change to a secure random string)
SESSION_SECRET=your-secure-session-secret